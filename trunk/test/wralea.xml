<?xml version="1.0" ?>
<openalea>
	<wraleapath value="."/>
	<wraleapath value="/usr/lib/python2.4/site-packages/openalea"/>
	<package name="Examples">
		<institutes>
			INRIA/CIRAD
		</institutes>
		<version>
			0.0.1
		</version>
		<description>
			Subgraphs.
		</description>
		<license>
			CECILL-C
		</license>
		<authors>
			SDK, CP
		</authors>
		<subgraph category="Examples" name="SubGraphExample">
			<description>
				Examples
			</description>
			<doc>
				
			</doc>
			<element factory_id="add" id="add_1" package_id="simpleop"/>
			<element factory_id="val" id="val_4" package_id="simpleop"/>
			<element factory_id="val" id="val_3" package_id="simpleop"/>
			<element factory_id="val" id="val_2" package_id="simpleop"/>
			<connect dst_id="add_1" dst_port="1" src_id="val_3" src_port="0"/>
			<connect dst_id="val_4" dst_port="0" src_id="add_1" src_port="0"/>
			<connect dst_id="add_1" dst_port="0" src_id="val_2" src_port="0"/>
		</subgraph>
	</package>
	<package name="simpleop">
		<institutes>
			INRIA/CIRAD
		</institutes>
		<version>
			0.0.1
		</version>
		<description>
			Simple operations.
		</description>
		<license>
			CECILL-C
		</license>
		<authors>
			SDK, CP
		</authors>
		<nodefactory category="Operations" name="add">
			<description>
				Addition
			</description>
			<doc>
				 Generic Addition
    Input 0 : First value to add
    Input 1 : Second value to add
    Output 0 : Addition result
    
			</doc>
			<node class="Add" module="simpleop"/>
			<widget class="None" module="None"/>
		</nodefactory>
		<nodefactory category="Operations" name="val">
			<description>
				Value
			</description>
			<doc>
				 Variable
    Input 0 : if connected, set the stored value
    Ouput 0 : transmit the stored value
    
			</doc>
			<node class="Value" module="simpleop"/>
			<widget class="None" module="None"/>
		</nodefactory>
	</package>
</openalea>
